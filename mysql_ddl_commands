SELECT User, Host FROM mysql.user;

CREATE USER 'animaker'@'localhost' IDENTIFIED BY 'animaker';
GRANT ALL PRIVILEGES ON *.* TO 'animaker'@'localhost';
FLUSH PRIVILEGES;

SELECT User, Host FROM mysql.user;

CREATE DATABASE animaker_db;

